        max-width: 100%;
        margin: 0 10%;
    }

     .example-tab-panel.is-active {
         display: block;
         max-width: 84%;
         margin: 0 auto;
     }
 }

 /* Carousel */
 .carousel-content {
     color: #fff;
     text-align: center;
     background: #3d3d3b;
     padding: 6%;
 }

 .carousel-content h2 {
     font-size: 37px;
     line-height: 50px;
 }

 .carousel-content h1 {
     font-size: 17px;
     color: #999;
 }

 .carousel-inner-wrap {
     list-style-type: none;
     display: flex;
     width: 100%;
     flex-direction: row;
     flex-wrap: wrap;
     flex-flow: wrap;
     justify-content: end;
     align-items: center;
     align-content: center;
 }

 .profile-table {
     width: 33%;
     margin: 0 auto;
     color: #fff;
     text-align: center;
     display: flex;
 }

 .profile-table td {
     display: block;
 }

 .profile-table h3 {
     font-size: 23px;
     margin: 0;
 }

 .profile-table h4 {
     text-transform: uppercase;
     margin: 0;
     font-size: 20px;
 }

 .profile-table p {
     color: #D1D1D7;
     margin: 5px 0;
 }

 .profile-slider-img {
     object-fit: cover;
     object-position: center center;
     width: 250px;
     height: 250px;
     border-radius: 135px;
 }

 * {
     box-sizing: border-box
 }

 .mySlides {
     display: none
 }

 img {
     vertical-align: middle;
 }

 /* Slideshow container */

 .slideshow-container {
     position: relative;
     margin: auto;
 }

 /* Next & previous buttons */

 .prev,
 .next {
     cursor: pointer;
     position: absolute;
     top: 50%;
     width: auto;
     padding: 16px;
     margin-top: -22px;
     color: white;
     font-weight: bold;
     font-size: 18px;
     transition: 0.6s ease;
     border-radius: 0 3px 3px 0;
     user-select: none;
 }

 /* Position the "next button" to the right */
 .next {
     right: 0;
     border-radius: 3px 0 0 3px;
 }

 /* On hover, add a black background color with a little bit see-through */
 .prev:hover,
 .next:hover {
     background-color: rgba(0, 0, 0, 0.8);
 }

 /* Caption text */
 .text {
     color: #f2f2f2;
     font-size: 15px;
     padding: 8px 12px;
     position: absolute;
     bottom: 8px;
     width: 100%;
     text-align: center;
 }

 /* Number text (1/3 etc) */
 .numbertext {
     color: #f2f2f2;
     font-size: 12px;
     padding: 8px 12px;
     position: absolute;
     top: 0;
 }

 /* The dots/bullets/indicators */

 .carousel__navigation {
     right: 0;
     bottom: 140px;
     left: 334px;
     text-align: center;
     width: 29%;
     position: relative;
     text-align-last: left;
 }

 .dot,
 .carousel__navigation-button {
     display: inline-block;
     width: 80px;
     height: 12px;
     background-color: #777;
     background-clip: content-box;
     border: 0.25rem solid transparent;
     font-size: 0;
     transition: background-color 0.6s ease;
     cursor: pointer;
 }

 .active,
 .carousel__navigation-button:hover {
     background-color: #ed1a3d;
 }

 /* Fading animation */

 .fade {
     -webkit-animation-name: fade;
     -webkit-animation-duration: 1.5s;
     animation-name: fade;
     animation-duration: 1.5s;
 }

 @-webkit-keyframes fade {
     from {
         opacity: .4
     }

     to {
         opacity: 1
     }
 }

 @keyframes fade {
     from {
         opacity: .4
     }

     to {
         opacity: 1
     }
 }


 /* On smaller screens, decrease text size */

 @media only screen and (max-width: 300px) {

     .prev,
     .next,
     .text {
         font-size: 11px
     }
 }


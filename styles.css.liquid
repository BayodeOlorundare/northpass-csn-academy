 /* Body */

 body {
     background: #ffffff;
 }

 /* Tabbed Course Selection */

 .example-tabs {
     width: 75%;
     margin: 0 auto 5%;
 }

 .join-course-btn,
 .example-tabs button,
 .learning-tabs button {
     background-color: white;
     border: 1px solid #ed1a3d;
     border-radius: 30px;
     padding: 10px 15px;
     text-decoration: none;
     -moz-transition: all 0.2s ease;
     -o-transition: all 0.2s ease;
     -webkit-transition: all 0.2s ease;
     transition: all 0.2s ease;
     cursor: pointer;
 }

 .example-tabs button:hover,
 .example-tabs button:focus,
 .learning-tabs button:hover,
 .learning-tabs button:focus {
     text-decoration: none;
     background-color: #ed1a3d;
     color: white;
     border: 1px solid #ed1a3d;
 }

 .example-tabs button.is-active,
 .is-active {
     background-color: #ed1a3d;
     color: white;
     border: 1px solid #ed1a3d;
 }

 .example-tab-panel {
     display: none;
     z-index: 1;
     padding: .5em;
     background-color: white;
     -moz-transition: all 0.5s ease;
     -o-transition: all 0.5s ease;
     -webkit-transition: all 0.5s ease;
     transition: all 0.5s ease;
     border: 1px solid #ffffff;
 }

 .example-tab-panel.is-active {
     display: block;
 }

 /* Course */

.np-homepage-featured-courses {
    justify-content: center;
    margin: 0 8%;
}

.np-card-ribbon {
    background: #07a9cb;
    color: #fff;
    font-size: 15px;
    font-weight: 700;
    line-height: 14px;
    padding: 5px 10px;
    position: absolute;
    right: 8px;
    text-transform: uppercase;
    top: 8px;
}

 .course-section h1 {
     font-size: 2em;
     margin: .67em 0;
     position: relative;
     max-width: 75%;
     margin: 25px auto 25px;
 }

 .course-card {
     max-width: 300px;
 }

 .course-error {
     display: none;
 }

 .row {
     justify-content: center;
 }

 @media (min-width: 769px) {
